<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lp.sidebar_master">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.Country1Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.Country2Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.Country4Activity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.Country3RvActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.Country3LvActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.Country5Activity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.Country6Activity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait" />
    </application>

</manifest>